    </item>
    <item>
        <shape >
            <solid android:color="@drawable/back_btn_normal" />
            <corners android:topLeftRadius="10.0px" android:topRightRadius="1.0px" android:bottomLeftRadius="10.0px" android:bottomRightRadius="1.0px"/>
        </shape>
    </item>
</selector>
