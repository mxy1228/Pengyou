package com.cyou.mrd.pengyou.entity;

public class PYUpdate {

	private int apkversion;//VersionCode
	private String apkdesc;//升级提示
	private String apkurl;//下载地址
	private String apkvername;
	public int getApkversion() {
		return apkversion;
	}
	public void setApkversion(int apkversion) {
		this.apkversion = apkversion;
	}
	public String getApkdesc() {
		return apkdesc;
	}
	public void setApkdesc(String apkdesc) {
		this.apkdesc = apkdesc;
	}
	public String getApkurl() {
		return apkurl;
	}
	public void setApkurl(String apkurl) {
		this.apkurl = apkurl;
	}
	public String getApkvername() {
		return apkvername;
	}
	public void setApkvername(String apkvername) {
		this.apkvername = apkvername;
	}
	
}
