package com.cyou.mrd.pengyou.entity;

public class SinaFriendItem {
	public static final String PENGYOU_USER = "1";
	public static final String SNS_USER = "2";
	private String uid;
	private String nickname;
	private String snsnicknm;
	private boolean isAttention = false;// 是否关注
	private boolean isInvisition = false;// 是否已邀请
	private int index;
	private String picture;
	private String snsusrimg;

	private String snsusrid;// SNs ID
	private String snsavatar;// sns icon
	private String avatar;// pengyou icon
	// public String isPengyouUser="1";//是否是朋游用户 1 为朋游用户 2不是 主要为了显示title做判断

	public String getUid() {
		return uid;
	}

	public void setUid(String uid) {
		this.uid = uid;
	}

	public String getSnsusrid() {
		return snsusrid;
	}

	public void setSnsusrid(String snsusrid) {
		this.snsusrid = snsusrid;
	}

	public String getSnsavatar() {
		return snsavatar;
	}

	public void setSnsavatar(String snsavatar) {
		this.snsavatar = snsavatar;
	}

	public String getAvatar() {
		return avatar;
	}

	public void setAvatar(String avatar) {
		this.avatar = avatar;
	}

	public String getNickname() {
		return nickname;
	}

	public void setNickname(String nickname) {
		this.nickname = nickname;
	}

	public String getSnsnicknm() {
		return snsnicknm;
	}

	public void setSnsnicknm(String snsnicknm) {
		this.snsnicknm = snsnicknm;
	}

	public boolean isAttention() {
		return isAttention;
	}

	public void setAttention(boolean isAttention) {
		this.isAttention = isAttention;
	}
	
	public boolean isInvisition() {
		return isInvisition;
	}

	public void setInvisition(boolean isInvisition) {
		this.isInvisition = isInvisition;
	}
	public int getIndex() {
		return index;
	}

	public void setIndex(int index) {
		this.index = index;
	}

	public String getPicture() {
		return picture;
	}

	public void setPicture(String picture) {
		this.picture = picture;
	}

	public String getSnsusrimg() {
		return snsusrimg;
	}

	public void setSnsusrimg(String snsusrimg) {
		this.snsusrimg = snsusrimg;
	}

}
